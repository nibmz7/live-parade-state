{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "target": "ES2019",
    "module": "ESNext",
    "lib": ["ES2019", "DOM"],
    "sourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "strict": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "outDir": "dev"
  },
  "files": ["./node_modules/firebase/index.d.ts"],
  "include": [
    "./model/**/*.ts",
    "./data/**/*.ts",
    "./data-firebase/**/*.ts",
    "./data-mock/**/*.ts",
    "./ui/**/*.ts",
    "./test/**/*.ts"
  ],
  "exclude": []
}
